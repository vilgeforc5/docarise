{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/docarise/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/docarise/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/docarise",
  "projects": {
    "crawler": {
      "type": "application",
      "root": "apps/crawler",
      "entryFile": "main",
      "sourceRoot": "apps/crawler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/crawler/tsconfig.app.json"
      }
    },
    "docarise": {
      "type": "application",
      "root": "apps/docarise",
      "entryFile": "main",
      "sourceRoot": "apps/docarise/src",
      "compilerOptions": {
        "tsConfigPath": "apps/docarise/tsconfig.app.json"
      }
    },
    "events": {
      "type": "library",
      "root": "libs/events",
      "entryFile": "index",
      "sourceRoot": "libs/events/src",
      "compilerOptions": {
        "tsConfigPath": "libs/events/tsconfig.lib.json"
      }
    },
    "transports": {
      "type": "library",
      "root": "libs/transports",
      "entryFile": "index",
      "sourceRoot": "libs/transports/src",
      "compilerOptions": {
        "tsConfigPath": "libs/transports/tsconfig.lib.json"
      }
    }
  }
}